    "source-code": [
        "package newSnippets;",
        "",
        "public class SignChecker {",
        "    private int number;",
        "",
        "    public SignChecker(int currentNumber) {",
        "        number = currentNumber;",
        "    }",
        "",
        "    public String check() {",
        "        String theSign = \"\";",
        "        if (number < 0) {",
        "            theSign = \"negative\";",
        "        } else if (number > 0) {",
        "            theSign = \"positive\";",
        "        } else {",
        "            theSign = \"null\";",
        "        }",
        "        return theSign;",
        "    }",
        "}"
    ]
}